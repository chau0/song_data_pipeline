    songplays }|--|{ songs : song_id
    artists ||--|{ songs : artist_id
    users ||--o{ songplays : user_id
    songplays ||--|| time : start_time
    users {
        int user_id
        string first_name
        string last_name
        string gender
        string level
    }
    songs {
        string song_id
        string title
        string artist_id
        int year
        float duration
    }
    artists {
        string artist_id
        string name
        string location
        float latitude
        float longitude
    }
    time {
        bigInt start_time
        int hour
        int day
        int week
        int month
        int year
        int weekday
    }
    songplays {
        string songplay_id
        bigInt start_time
        int user_id
        string level
        string song_id
        string artist_id
        int session_id
        string location
        string user_agent  
    }